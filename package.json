{
  "name": "crawl-system",
  "version": "1.0.0",
  "description": "Crawl system based on MoleculerJS",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "dev": "ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts",
    "start": "ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts",
    "cli": "moleculer connect Redis",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "homepage": "#readme",
  "author": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.4.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.31",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "bcrypt": "^5.0.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "moleculer-repl": "^0.7.0",
    "prettier": "^2.6.2",
    "tsconfig-paths": "^4.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0"
  },
  "dependencies": {
    "@ourparentcenter/moleculer-decorators-extended": "^1.1.1",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.22.4",
    "cross-env": "^7.0.3",
    "csv-parse": "^4.16.0",
    "dotenv": "^16.0.1",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "ioredis": "^4.27.7",
    "json2typescript": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.21",
    "moleculer-bull": "^0.2.8",
    "moleculer-db": "^0.8.17",
    "moleculer-db-adapter-mongo": "^0.4.14",
    "moleculer-db-adapter-mongoose": "^0.8.12",
    "mongodb": "^4.5.0",
    "mongoose": "^5.13.6",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "resilient": "^0.4.0",
    "ts-results": "^3.3.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">= 16.x.x"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
